#page-content {
	.content { background: #fff; }

	.search-content {
		h2 {
			font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif; 
			font-weight: 300;
			color: #0055a7;
		}
	}

	.find-sears {

			h2 { margin-bottom: 0; }

			h3 {
				color: #499cb5;
				font-size: 10px;
			}
		}

		.find-sears-search {

			input { 
				float: left;
				margin: 0 8px 0 0;
				padding: 6.5px;
				font-family: arial;
				font-size: 11px;

				&.zip-city-state { width: 250px; }
			}

			::-webkit-input-placeholder { /* WebKit browsers */
	  		font-weight: normal;
			}
			:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
				font-weight: normal;
			}
			::-moz-placeholder { /* Mozilla Firefox 19+ */
				font-weight: normal;
			}
			:-ms-input-placeholder { /* Internet Explorer 10+ */
				font-weight: normal;
			}

			.go {
				cursor: pointer;
				background: #f7c433;
				width: 16px;
				height: 12px;
				padding: 7px 5px;
				color: #fff;
				font-size: 10px;
				border-radius: 20px;
				float: left;
				margin: 1.5px 8px 0 0;
				font-weight: bold;

				&:hover { background: #eb833a; }
			}
		}
}

