	.find-sears {

			h2 { margin-bottom: 0; }

			h3 {
				color: #499cb5;
				font-size: 10px;
			}
		}

		.find-sears-search {

			input { 
				float: left;
				margin: 0 8px 0 0;
				padding: 6.5px;
				font-family: arial;
				font-size: 11px;

				&.zip-city-state { width: 250px; }
