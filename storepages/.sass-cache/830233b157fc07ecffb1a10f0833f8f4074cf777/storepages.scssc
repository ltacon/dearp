		.sears-family { background: url('../images/sears-family.png') no-repeat; }
		.sears-garden { background: url('../images/sears-garden.png') no-repeat; }
		.sears-outdoors { background: url('../images/sears-outdoors.png') no-repeat; }
		.sears-fashion { background: url('../images/sears-fashion.png') no-repeat; }

	}

	.find-sears {
		position: absolute;
		top: 100px;
		left: 25.5px;
	}

	.find-sears-search {
		float: left;
		width: 775px;
	}

	.sears-nearby { 
		font-size: 11px;
		width: 100%;
		background: url('../images/sears-nearby-bg.png') repeat; 
		padding: 15px 25.5px 20px;
		position: absolute;
		top: 283px;

		.sears-nearby-content {
			width: 980px;
			margin: 0 auto;
		}

		.sears-store {
			float: left;
			padding: 0 35px;
			
			&.sears-store-1 { padding-left: 0; }
			&.sears-store-4 { padding-right: 0; }

			h1{ 
				font-weight: bold;
				font-size: 12px;
				position: relative;

				a { 
					text-decoration: none;
					color: #0055a7; 

					&:hover { color: #E31837; }
				}

				.local-website {
					position: absolute;
					top: 0px;
					right: -17px;
				}
			}

			h4 { 
				font-size: 10px;
				font-weight: bold;
				margin-bottom: 8px;
				color: #E31837;
				text-transform: uppercase;
			}

			.store-foot {
				.see-map { 
					text-decoration: none;
					color: #067bb5;
				}
			}
		}
	}
}

